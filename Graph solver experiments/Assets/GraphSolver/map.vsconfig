import epackage "models/map.ecore"
import viatra "queries/map.vql"

generate {
	metamodel = {package map}
	partial-model = { "instance.xmi"}
	constraints = { package queries }
	

	solver = ViatraSolver
	
	scope = {
		#node = 33..100
	}
	
	number = 1
	runs = 1
	config = {
		log-level = normal
	}
	
	debug = "debug"
	statistics = "statistics/"
	output = "output/"
}